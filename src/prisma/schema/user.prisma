//run after changes to model >> npx prisma generate << to update prisma client
//npx prisma migrate dev --name add-a-name << to update data base

model User {
    id        String   @id @default(cuid())
    email     String
    password  String
    isAdmin   Boolean? @default(false)
    recipes   Recipe[]
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
