//run after changes to model >> npx prisma generate << to update prisma client

model Recipe {
    id          String           @id @default(cuid())
    title       String
    prepTime    Int?
    description String?
    creatorId   String
    creator     User             @relation(fields: [creatorId], references: [id])
    ingredients RecipeFoodItem[]
    createdAt   DateTime         @default(now())
    updatedAt   DateTime         @updatedAt
}
