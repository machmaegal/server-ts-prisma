//run after changes to model to update database: npx prisma migrate dev --name <add-a-name> 

model RecipeFoodItem {
    id           String   @id @default(cuid())
    recipe       Recipe   @relation(fields: [recipeId], references: [id])
    recipeId     String
    ingredient   FoodItem @relation(fields: [ingredientId], references: [id])
    ingredientId String

    @@unique([recipeId, ingredientId])
}
