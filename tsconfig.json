{
    "compilerOptions": {
        "target": "ES2020",
        "moduleDetection": "force",
        "module": "commonjs",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "skipLibCheck": true
    },
    "include": [
        "src/**/*"
    ]
}